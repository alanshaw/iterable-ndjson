    "async",
    "iterator",
    "iterable"
  ],
  "author": "Alan Shaw",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alanshaw/iterable-ndjson/issues"
  },
  "homepage": "https://github.com/alanshaw/iterable-ndjson#readme",
  "devDependencies": {
    "nyc": "^13.2.0",
    "standard": "^12.0.1",
    "tape": "^4.10.1",
    "tape-puppet": "^0.1.7",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  },
  "dependencies": {
    "string_decoder": "^1.2.0"
  }
}
